<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity DrawingItem.
    -->
    <changeSet id="20220609080500-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="shape_asset_id"
                                 baseTableName="tb_drawingitem"
                                 constraintName="fk_tb_drawingitem__shape_asset_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_shapeasset"/>

        <addForeignKeyConstraint baseColumnNames="resource_id"
                                 baseTableName="tb_drawingitem"
                                 constraintName="fk_tb_drawingitem__resource_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_resource"/>

        <addForeignKeyConstraint baseColumnNames="drawing_id"
                                 baseTableName="tb_drawingitem"
                                 constraintName="fk_tb_drawingitem__drawing_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_drawing"/>
    </changeSet>
</databaseChangeLog>
