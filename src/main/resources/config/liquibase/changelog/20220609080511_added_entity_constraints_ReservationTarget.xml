<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity ReservationTarget.
    -->
    <changeSet id="20220609080511-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="resource_id"
                                 baseTableName="tb_reservationtarget"
                                 constraintName="fk_tb_reservationtarget__resource_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_resource"/>

        <addForeignKeyConstraint baseColumnNames="reservation_id"
                                 baseTableName="tb_reservationtarget"
                                 constraintName="fk_tb_reservationtarget__reservation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tb_reservation"/>
    </changeSet>
</databaseChangeLog>
